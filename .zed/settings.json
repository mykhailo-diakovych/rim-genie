{
  "context_servers": {
    "context7": {
      "source": "custom",
      "command": "npx",
      "args": ["-y", "@upstash/context7-mcp"],
      "env": {}
    },
    "shadcn": {
      "source": "custom",
      "command": "npx",
      "args": ["-y", "shadcn@latest", "mcp"],
      "env": {}
    },
    "better-auth": {
      "source": "custom",
      "type": "http",
      "url": "https://mcp.inkeep.com/better-auth/mcp",
      "headers": {}
    }
  }
}
